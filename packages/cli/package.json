{
  "name": "@stegapng/cli",
  "version": "1.1.0",
  "description": "A CLI for stegapng",
  "bin": {
    "stega": "dist/index.js"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "fmt": "prettier --write src",
    "prepack": "pnpm run fmt && pnpm run build && pnpm run test",
    "pkg": "pkg . --out-path=bin --targets node18-linux-x64,node18-macos-x64,node18-win-x64 --overwrite",
    "test": "spec/test.sh"
  },
  "files": [
    "dist"
  ],
  "pkg": {
    "assets": [
      "node_modules/@img/**/*"
    ]
  },
  "keywords": [
    "steganography",
    "png",
    "cli"
  ],
  "author": "Wes Roberts <takq@a.zinc.email>",
  "license": "MIT",
  "dependencies": {
    "@stegapng/core": "workspace:*",
    "commander": "^11.1.0",
    "sharp": "^0.33.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.3",
    "pkg": "^5.8.1",
    "typescript": "^5.3.2"
  }
}
